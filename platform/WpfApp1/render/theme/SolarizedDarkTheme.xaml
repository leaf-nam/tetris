<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Base -->
    <SolidColorBrush x:Key="Background" Color="#002B36"/>
    <SolidColorBrush x:Key="Panel" Color="#073642"/>
    <SolidColorBrush x:Key="Foreground" Color="#EEE8D5"/>
    <SolidColorBrush x:Key="Comment" Color="#586E75"/>

    <!-- Primary -->
    <SolidColorBrush x:Key="Cyan" Color="#2AA198"/>
    <SolidColorBrush x:Key="Green" Color="#859900"/>
    <SolidColorBrush x:Key="Orange" Color="#CB4B16"/>
    <SolidColorBrush x:Key="Pink" Color="#D33682"/>
    <SolidColorBrush x:Key="Purple" Color="#6C71C4"/>
    <SolidColorBrush x:Key="Red" Color="#DC322F"/>
    <SolidColorBrush x:Key="Yellow" Color="#B58900"/>

    <!-- Accents -->
    <SolidColorBrush x:Key="Accent1" Color="#B58900"/>
    <SolidColorBrush x:Key="Accent2" Color="#CB4B16"/>
    <SolidColorBrush x:Key="Accent3" Color="#859900"/>
    <SolidColorBrush x:Key="Accent4" Color="#2AA198"/>
    <SolidColorBrush x:Key="Accent5" Color="#268BD2"/>
    <SolidColorBrush x:Key="Accent6" Color="#6C71C4"/>
    <SolidColorBrush x:Key="Accent7" Color="#D33682"/>

</ResourceDictionary>
