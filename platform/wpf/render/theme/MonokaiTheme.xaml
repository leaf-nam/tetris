<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Base -->
    <SolidColorBrush x:Key="Background" Color="#272822"/>
    <SolidColorBrush x:Key="Panel" Color="#3E3D32"/>
    <SolidColorBrush x:Key="Foreground" Color="#F8F8F2"/>
    <SolidColorBrush x:Key="Comment" Color="#75715E"/>

    <!-- Primary -->
    <SolidColorBrush x:Key="Cyan" Color="#66D9EF"/>
    <SolidColorBrush x:Key="Green" Color="#A6E22E"/>
    <SolidColorBrush x:Key="Orange" Color="#FD971F"/>
    <SolidColorBrush x:Key="Pink" Color="#F92672"/>
    <SolidColorBrush x:Key="Purple" Color="#AE81FF"/>
    <SolidColorBrush x:Key="Red" Color="#F92672"/>
    <SolidColorBrush x:Key="Yellow" Color="#E6DB74"/>

    <!-- Accents -->
    <SolidColorBrush x:Key="Accent1" Color="#FD971F"/>
    <SolidColorBrush x:Key="Accent2" Color="#E6DB74"/>
    <SolidColorBrush x:Key="Accent3" Color="#A6E22E"/>
    <SolidColorBrush x:Key="Accent4" Color="#66D9EF"/>
    <SolidColorBrush x:Key="Accent5" Color="#AE81FF"/>
    <SolidColorBrush x:Key="Accent6" Color="#F92672"/>
    <SolidColorBrush x:Key="Accent7" Color="#F8F8F2"/>

</ResourceDictionary>
