<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Base -->
    <SolidColorBrush x:Key="Background" Color="#2E3440"/>
    <SolidColorBrush x:Key="Panel" Color="#3B4252"/>
    <SolidColorBrush x:Key="Foreground" Color="#ECEFF4"/>
    <SolidColorBrush x:Key="Comment" Color="#616E88"/>

    <!-- Primary -->
    <SolidColorBrush x:Key="Cyan" Color="#88C0D0"/>
    <SolidColorBrush x:Key="Green" Color="#A3BE8C"/>
    <SolidColorBrush x:Key="Orange" Color="#D08770"/>
    <SolidColorBrush x:Key="Pink" Color="#B48EAD"/>
    <SolidColorBrush x:Key="Purple" Color="#B48EAD"/>
    <SolidColorBrush x:Key="Red" Color="#BF616A"/>
    <SolidColorBrush x:Key="Yellow" Color="#EBCB8B"/>

    <!-- Accents -->
    <SolidColorBrush x:Key="Accent1" Color="#EBCB8B"/>
    <SolidColorBrush x:Key="Accent2" Color="#D8DEE9"/>
    <SolidColorBrush x:Key="Accent3" Color="#ECEFF4"/>
    <SolidColorBrush x:Key="Accent4" Color="#8FBCBB"/>
    <SolidColorBrush x:Key="Accent5" Color="#81A1C1"/>
    <SolidColorBrush x:Key="Accent6" Color="#5E81AC"/>
    <SolidColorBrush x:Key="Accent7" Color="#A3BE8C"/>

</ResourceDictionary>
